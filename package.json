    "eslint": "^7.10.0",
    "eslint-config-alloy": "^3.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.2",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "json-loader": "^0.5.7",
    "less": "^3.12.2",
